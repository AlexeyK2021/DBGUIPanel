<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ru.alexeyk2021.dbguipanel.LoginPage.LoginActivityController">
    <TextField promptText="Логин" fx:id="login_text"/>
    <PasswordField promptText="Пароль" fx:id="password_text"/>
    <Button text="Login" fx:id="enter_btn" onAction="#enter"/>
    <Button fx:id="loginAsAdmin" onAction="#loginAsAdmin" text="Login as admin"/>
    <Label visible="false" fx:id="is_data_correct"/>
</VBox>
